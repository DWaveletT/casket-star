{
  "name": "casket-star",
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/casket-star.umd.cjs",
  "module": "./dist/casket-star.js",
  "exports": {
    ".": {
      "import": "./dist/casket-star.js",
      "require": "./dist/casket-star.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./themes/": "./dist/themes/"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build-casket && pnpm run build-themes",
